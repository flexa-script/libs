namespace flx;

const NEW_LINE: string = "\n";

fun _print_values(...values: any) {
	println();
	foreach (var v in values) {
		print(v);
	}
}

fun printls(...values: any) {
	println();
	_print_values(values);
}

fun printlb(...values: any) {
	println();
	_print_values(values);
	println();
}
